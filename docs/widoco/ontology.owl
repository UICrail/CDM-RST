<?xml version="1.0"?>
<rdf:RDF xmlns="https://cdm.ovh/rst/core#"
     xml:base="https://cdm.ovh/rst/core"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://cdm.ovh/rst/core">
        <owl:imports rdf:resource="https://cdm.ovh/rst/cons/0.1/cons.ttl"/>
        <owl:imports rdf:resource="https://cdm.ovh/rst/typo/0.2/typo.ttl"/>
        <dc:description>Imports all CDM rolling stock-related ontologies</dc:description>
        <dc:license rdf:resource="https://interoperable-europe.ec.europa.eu/sites/default/files/custom-page/attachment/2020-03/EUPL-1.2%20EN.txt"/>
        <dc:title>Aggregated Rolling stock ontologies</dc:title>
        <rdfs:comment>Current direct imports: rolling stock consist and typology ontologies.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/license"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#boolean -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#boolean"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#integer -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#integer"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://cdm.ovh/rst/cons/cons#coupledTo -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#coupledTo">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rst/cons/cons#facesExtremity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:comment xml:lang="en">Symmetric property &quot;coupled to&quot; relates two extremities of two distinct Rolling stock items (hence the &quot;to&quot; in the label). The property is expressed irrespectively of the type of coupling (mechanical, virtual) or coupler (UIC, Scharfenberg...).</rdfs:comment>
        <rdfs:label>coupled to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#extremity_1 -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#extremity_1">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rst/cons/cons#hasExtremity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <owl:propertyDisjointWith rdf:resource="https://cdm.ovh/rst/cons/cons#extremity_2"/>
        <rdfs:comment xml:lang="en">&quot;extremity 1&quot; (actual designation may vary) of a Rolling stock.</rdfs:comment>
        <rdfs:label xml:lang="en">extremity_1</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#extremity_2 -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#extremity_2">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rst/cons/cons#hasExtremity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">&quot;extremity 2&quot; of a piece of Rolling stock. Designation may vary.</rdfs:comment>
        <rdfs:label xml:lang="en">extremity_2</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#facesExtremity -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#facesExtremity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rst/cons/cons#Extremity"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rst/cons/cons#Extremity"/>
        <rdfs:comment xml:lang="en">object is the extremity of another Rolling stock that the subject extremity faces.</rdfs:comment>
        <rdfs:label xml:lang="en">faces extremity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#frontExtremity -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#frontExtremity">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rst/cons/cons#hasExtremity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rst/cons/cons#ListedRollingStock"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rst/cons/cons#Extremity"/>
        <rdfs:label xml:lang="en">front extremity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#hasExtremity -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#hasExtremity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rst/cons/cons#RollingStock"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rst/cons/cons#Extremity"/>
        <rdfs:label xml:lang="en">has extremity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#hasHeadRollingStock -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#hasHeadRollingStock">
        <rdfs:subPropertyOf rdf:resource="https://cdm.ovh/rst/cons/cons#includesRollingStock"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rst/cons/cons#Formation"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rst/cons/cons#ListedRollingStock"/>
        <rdfs:comment xml:lang="en">Each formation has exactly one &quot;First&quot; rolling stock. This notion of &quot;first&quot; is only related to the rolling stock, not however to the direction of travel which may vary (general case of multiple units or reversible trains) or not (case of hauled freight trains). Please also not that the direction of movement is yet another notion (think of shunting).</rdfs:comment>
        <rdfs:label xml:lang="en">has head rolling stock</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#hasNextRollingStock -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#hasNextRollingStock">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/list#hasNext"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rst/cons/cons#ListedRollingStock"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://cdm.ovh/rst/cons/cons#ListedRollingStock"/>
                    <rdf:Description rdf:about="https://w3id.org/list#EmptyList"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Indicates which Rolling stock comes next in a Consist.</rdfs:comment>
        <rdfs:label xml:lang="en">has next rolling stock</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#includesRollingStock -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#includesRollingStock">
        <owl:inverseOf rdf:resource="https://cdm.ovh/rst/cons/cons#partOfFormation"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rst/cons/cons#Formation"/>
        <rdfs:range rdf:resource="https://cdm.ovh/rst/cons/cons#RollingStock"/>
        <rdfs:comment xml:lang="en">This property describes the composition of a Formation. A Formation may incude any rolling stock (i.e. vehicles or formations). Irreflexive property: a formation cannot include itself; in other words, it may only have proper parts.</rdfs:comment>
        <rdfs:label xml:lang="en">includes Rolling stock</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#partOfFormation -->


    <owl:ObjectProperty rdf:about="https://cdm.ovh/rst/cons/cons#partOfFormation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:comment xml:lang="en">Inverse of &quot;includes Rolling Stock&quot;. A functional property: a rolling stock can only be part of one formation. Irreflexive property: A formation cannot be part of itself.</rdfs:comment>
        <rdfs:label xml:lang="en">part of formation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/list#hasNext -->


    <owl:ObjectProperty rdf:about="https://w3id.org/list#hasNext">
        <rdfs:label xml:lang="en">has next</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://cdm.ovh/rsm/rolling_stock/rolling_stock_typology#hasFixedConsist -->


    <owl:DatatypeProperty rdf:about="http://cdm.ovh/rsm/rolling_stock/rolling_stock_typology#hasFixedConsist">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rst/cons/cons#Formation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Train formation, the consist of which cannot be changed while in operations as it would require interventions in a workshop.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://cdm.ovh/rst/cons/cons#positionInFormation -->


    <owl:DatatypeProperty rdf:about="https://cdm.ovh/rst/cons/cons#positionInFormation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://cdm.ovh/rst/cons/cons#RollingStock"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">Starting with 1.</rdfs:comment>
        <rdfs:label xml:lang="en">position in formation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://cdm.ovh/rst/cons/cons#Extremity -->


    <owl:Class rdf:about="https://cdm.ovh/rst/cons/cons#Extremity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://cdm.ovh/rst/cons/cons#hasExtremity"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">denotes the extremity of a rolling stock. The extremity designation is either set at design and manufacturing time (e.g. extremity1, extremity 2 in the case of a wagon), or rests on other conventions (e.g. extremities of a variable-consist trainset).</rdfs:comment>
        <rdfs:label xml:lang="en">Extremity</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/cons/cons#Formation -->


    <owl:Class rdf:about="https://cdm.ovh/rst/cons/cons#Formation">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#RollingStock"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://cdm.ovh/rst/cons/cons#includesRollingStock"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://cdm.ovh/rst/cons/cons#Vehicle"/>
        <rdfs:comment xml:lang="en">A sequence of vehicles (or other formations) that are coupled in some way (mechanically or virtually).</rdfs:comment>
        <rdfs:label xml:lang="en">Formation</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/cons/cons#ListedRollingStock -->


    <owl:Class rdf:about="https://cdm.ovh/rst/cons/cons#ListedRollingStock">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#RollingStock"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/list#OWLList"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://cdm.ovh/rst/cons/cons#hasNextRollingStock"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://w3id.org/list#EmptyList"/>
        <rdfs:comment xml:lang="en">Rolling stock that may have a predecessor or successor in a formation.</rdfs:comment>
        <rdfs:label xml:lang="en">Listed rolling stock</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/cons/cons#RollingStock -->


    <owl:Class rdf:about="https://cdm.ovh/rst/cons/cons#RollingStock">
        <rdfs:comment xml:lang="en">Base type for rolling stock. No direct instances are expected.</rdfs:comment>
        <rdfs:label xml:lang="en">Rolling stock</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/cons/cons#Vehicle -->


    <owl:Class rdf:about="https://cdm.ovh/rst/cons/cons#Vehicle">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#RollingStock"/>
        <rdfs:comment xml:lang="en">Common definition in Directives 2016/797, 2016/798:
 a railway vehicle suitable for circulation on wheels on railway lines, with or without traction; a vehicle is composed of one or more structural and functional subsystems

ERA vocabulary 3.0.0: A specific vehicle or wagon able to operate over railway lines.</rdfs:comment>
        <rdfs:comment xml:lang="en">Considered to be the atomic part of a piece of rolling stock, although disassembly may be carried out in special circumstances. Example: a TALGO or TGV trailer does not rest on two axles resp. bogies, but has a distinct carbody and it considered a vehicle (called &quot;trailer&quot;).  Further example: an articulated wagon (3-axle, e.g. carrying cars) is generally considered a single vehicle although it has two distinct chassis. So the distinction vehicle / formation is conventional; being &quot;atomic&quot; in operations is a necessary condition, but not a sufficient one to be called vehicle.</rdfs:comment>
        <rdfs:label xml:lang="en">Vehicle</rdfs:label>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#Coach -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#Coach">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Vehicle"/>
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#PassengerCarryingRollingStock"/>
        <rdfs:comment xml:lang="en">TSI LOCPAS 2020:
a vehicle without traction in a fixed or variable formation capable of carrying passengers (by extension, requirements specified to apply to coaches in this TSI are deemed to apply also to restaurant cars, sleeping cars, couchettes cars, etc.)

Note: was &quot;passenger carriage&quot; in earlier versions.</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#FreightCarryingRollingStock -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#FreightCarryingRollingStock">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#RollingStockFeature"/>
        <rdfs:comment xml:lang="en">Feature of Rolling stock: ability to carry freight, by design.</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#Locomotive -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#Locomotive">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Vehicle"/>
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#MotoredRollingStock"/>
        <rdfs:comment xml:lang="en">Powered vehicle having a driving desk and can be used standalone (e.g. for shunting or ferrying movements), but is normally used for hauling other vehicles or formations.</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#MotorCar -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#MotorCar">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Vehicle"/>
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#MotoredRollingStock"/>
        <rdfs:comment xml:lang="en">Powered vehicle that cannot be used standalone, as can be found in trainset (EMUs or DMUs).
May or may not carry passengers.</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#MotoredRollingStock -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#MotoredRollingStock">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#RollingStockFeature"/>
        <rdfs:comment xml:lang="en">Feature of rolling stock that is able by design to exert tractive effort on wheels or (rarely) cogwheels.</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#OnTrackMachine -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#OnTrackMachine">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Vehicle"/>
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#MotoredRollingStock"/>
        <rdfs:comment xml:lang="en">a motored railway vehicle used for infrastructure maintenance</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#PassengerCarryingRollingStock -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#PassengerCarryingRollingStock">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#RollingStockFeature"/>
        <rdfs:comment xml:lang="en">Feature of Rolling stock: ability to carry freight, by design.</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#RailCar -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#RailCar">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Vehicle"/>
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#MotoredRollingStock"/>
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#PassengerCarryingRollingStock"/>
        <rdfs:comment xml:lang="en">Wikipedia (12/3/2024): self-propelled railway vehicle designed to transport passengers.
Equiv. to FR: Autorail, DE: Schienenbus (narrower meaning: two axles only).
Note however the numerous variants and ambiguities.</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#Rake -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#Rake">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Formation"/>
        <rdfs:comment xml:lang="en">Any non-powered formation. Examples:
wagon rake;
(fixed consist) set of TGV trailers;
(fixed or variable consist) rake of coaches.</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#RoadRailVehicle -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#RoadRailVehicle">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Vehicle"/>
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#FreightCarryingRollingStock"/>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#RoadVehicleRake -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#RoadVehicleRake">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Formation"/>
        <rdfs:comment xml:lang="en">From TSI WAG 2023, &quot;unit&quot;, 3rd entry:
separate rail bogies connected to compatible road vehicle(s), the combination of which forms a rake of a rail compatible system.</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#RollingStockFeature -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#RollingStockFeature"/>
    


    <!-- https://cdm.ovh/rst/typo/typo#SlaveLocomotive -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#SlaveLocomotive">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Vehicle"/>
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#MotoredRollingStock"/>
        <rdfs:comment xml:lang="en">Powered vehicle without driving desk, with energy possibly fed from master locomotive ( = subclass &quot;slug&quot;).
Also called &quot;truck&quot; in France, e.g. TC61000.
Do not confuse with master/slave configuration of two normal locomotives (&quot;multiple working&quot; in the UK).</rdfs:comment>
    </owl:Class>
    


    <!-- https://cdm.ovh/rst/typo/typo#Trailer -->


    <owl:Class rdf:about="https://cdm.ovh/rst/typo/typo#Trailer">
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/cons/cons#Vehicle"/>
        <rdfs:subClassOf rdf:resource="https://cdm.ovh/rst/typo/typo#PassengerCarryingRollingStock"/>
        <rdfs:comment xml:lang="en">non-traction, passenger-carrying vehicle that is part of a trainset or of a fixed-consist rake, such as TGV trailer or ICE3 trailer or Talgo trailer.</rdfs:comment>
